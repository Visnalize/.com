main
  overflow-x hidden

img[alt="win7simu"]
  width 45px
  height 45px
  vertical-align bottom

.banner
  margin 1rem 0
  width 100%

.embed-responsive
  iframe
    display block
    margin auto
    max-width 100%

.contains-task-list
  list-style none
  padding 0

  input
    margin 0 8px 0 0
    transform scale(1.2)

@media (max-width: 719px)
  .nav-item > .dropdown-wrapper .nav-dropdown
    box-shadow: none

.nav-item:nth-child(2)::before
  content: ''
  position: absolute
  z-index -1
  top: 50%
  left: 30%
  width: 64px
  height: 64px
  background: url('data:image/svg+xml;base64,') no-repeat
  background-size: 100%
