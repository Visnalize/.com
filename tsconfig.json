{
  "include": [
    "**/*.ts",
    "**/*.vue",
    "**/*.md",
    ".content/**/*",
    ".vitepress/theme/**/*",
    ".vitepress/config/**/*",
    ".vitepress/plugins/**/*"
  ],
  "vueCompilerOptions": {
    "vitePressExtensions": [".md"]
  },
  "compilerOptions": {
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./*"],
      "@utils/*": ["./.vitepress/theme/utils/*"],
      "@composables/*": ["./.vitepress/theme/composables/*"],
      "@components/*": ["./.vitepress/theme/components/*"]
    }
  }
}
